<?xml version="1.0" encoding="UTF-8"?>
<!--
    Document   : puntosventa.xml
    Author     : Srok Desarrollos
    Description:
        Estructura para puntos de venta
-->
<table>
    <column>
        <name>codpv</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <column>
        <name>descripcion</name>
        <type>character varying(100)</type>
    </column>
     <column>
        <name>tipo</name>
        <type>integer</type>
        <null>NO</null>
    </column>
    <constraint>
        <name>pv_pkey</name>
        <type>PRIMARY KEY (codpv)</type>
    </constraint>
</table>
